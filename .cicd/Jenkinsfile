pipeline {
  agent none
  parameters {
    string(name: 'BRANCH', defaultValue: 'develop', description: '')
  }
  
  stages {
    stage('Testing') {
       agent {
        label 'built-in'   
       }
      steps {
        script {
          pullRequest.labels.each{
            if ($it =~ "*-RT") {
              echo "Regression Test label"
            }  
          }
          
          sh 'echo "I ran as expected!"'
        }    
      }
    }
  }
}
