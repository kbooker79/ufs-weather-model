pipeline {
  agent none
  parameters {
    string(name: 'BRANCH', defaultValue: 'develop', description: '')
  }
  
  stages {
    stage('Testing') {
       agent {
        label 'built-in'   
       }
      steps {
        script {
          for (label in pullRequest.labels) {
            if ((label.matches("(.*)-RT")) == "true") {
               println(label.matches"(.*)-RT"));
               println("I am a regression test label.");
            } 
          } 
          sh 'echo "I ran as expected!"'
        }    
      }
    }
  }
}
