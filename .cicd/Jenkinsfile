pipeline {
  agent none
  parameters {
    string(name: 'BRANCH', defaultValue: 'develop', description: '')
  }
  
  stages {
    stage('Testing') {
       agent {
        label 'built-in'   
       }
      steps {
        script {
          for (label in pullRequest.labels) {
             if (label ==~ "-RT") 
                echo "I am a regression test."
          } 
          sh 'echo "I ran as expected!"'
        }    
      }
    }
  }
}
