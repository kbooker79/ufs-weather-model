pipeline {
  agent none
  parameters {
    string(name: 'BRANCH', defaultValue: 'develop', description: '')
  }
  
  stages {
    stage('Testing') {
       agent {
        label 'built-in'   
       }
      steps {
        script {
          for (label in pullRequest.labels) {
            if ((label.matches("(.*)-RT"))) {
              //println(label.matches("(.*)-RT")); 
              env.CREATE_BASELINES = false
            } 
          } 
          sh 'echo "I ran as expected!"'
          sh 'echo "The value of CREATE_BASELINES is ${CREATE_BASELINES}"'
        }    
      }
    }
  }
}
