pipeline {
  agent none
  parameters {
    string(name: 'BRANCH', defaultValue: 'develop', description: '')
  }
  
  stages {
    stage('Start Parallelworks Cluster') {
       agent {
        label 'built-in'   
       }
      steps {
        script {
          pullRequest.labels.each{
            echo "label: $it"
         }
          sh 'echo "I ran as expected!"'
        }    
      }
    }
  }
}
