pipeline {
  agent none
  parameters {
    string(name: 'BRANCH', defaultValue: 'develop', description: '')
  }
  
  stages {
    stage('Testing') {
       agent {
        label 'built-in'   
       }
      steps {
        script {
          for (label in pullRequest.labels) {
            if ((label.matches("(.*)-BL"))) {
              env.CREATE_BASELINES = true 
              // adding a comment
            } 
          } 
          sh '''
          if [[ "${CREATE_BASELINES}" = true ]] 
          then
          echo "Creating baselines for regressions tests."
          fi
          '''
        }    
      }
    }
  }
}
